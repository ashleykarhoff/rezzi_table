<p><% if flash[:errors] %></p>
<% flash[:errors].each do |msg| %>
<%= msg %>
<% end %>
<% end %>

<h1>Edit Your Review </h1>

<h1><%= @review.restaurant_name %></h1>

<%= form_with model: @review do |f| %>

<%= f.label "rating" %>
<%= f.number_field :rating %><br><br>

<%= f.label "description" %>
<%= f.text_area :description %><br><br>

<%= f.submit %>
<% end %>
